BODY 
{background-image: url(/results_system_v2/common/bglogo.jpg); font-family: arial; font-size: 10pt }
A:link {color: #0000FF}
A:visited {color: #OO00FF}
H1 {font-size: 24pt; font-family: arial}
H2 {font-size: 18pt; font-family: arial}
H3 {font size: 14pt; font-family: arial}

TABLE.REP { background-color:white; border-collapse:collapse; border-spacing:0px; font-family: arial; font-size:10pt; }
TABLE.REP TR { text-align: center; }
TABLE.REP TH.teamcol { text-align:left; white-space: nowrap; background-color: #DFDFDF; }
TABLE.REP TD.teamcol { text-align: left; white-space: nowrap; background-color: #DFDFDF; }

TABLE.fix { background-color:white; border-collapse:collapse; border-spacing:0px; font-family: arial; font-size:10pt; }
TABLE.fix TR { text-align: center; }
TABLE.fix TH.teamcol { text-align: left; white-space: nowrap; background-color: #DFDFDF; }
TABLE.fix TD.teamcol { text-align: left; white-space: nowrap; background-color: #DFDFDF; }
TABLE.fix TH.perfcol { text-align: left; }
TABLE.fix TD.perfcol { text-align: left; }
